  </servlet>

  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>
  
  <jsp-config>
  	<jsp-property-group>
  		<url-patterm>*.jsp</url-patterm>
  		<el-ignored>false</el-ignored>
  		<scripting-invalid>true</scripting-invalid>
  	</jsp-property-group>
