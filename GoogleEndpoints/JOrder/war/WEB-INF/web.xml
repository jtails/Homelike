  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value>mx.jtails.homelike.model.endpoints.CProductoEndpoint,mx.jtails.homelike.model.endpoints.CantidadPagoEndpoint,mx.jtails.homelike.model.endpoints.CuentaEndpoint,mx.jtails.homelike.model.endpoints.PedidoEndpoint,mx.jtails.homelike.model.endpoints.ProductoEndpoint,mx.jtails.homelike.model.endpoints.ProveedorEndpoint,mx.jtails.homelike.model.endpoints.ServicioEndpoint,mx.jtails.homelike.model.endpoints.SugerenciaspEndpoint,mx.jtails.homelike.model.endpoints.TpedidoEndpoint</param-value>
